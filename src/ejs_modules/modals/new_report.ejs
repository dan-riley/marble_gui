<div class="modal" id="NewReportModal">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Artifact Submit</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <div class="form-group">
                    <!-- This is for selecting the artifact type -->
                    <select class="custom-select" id="edit_type">

                    </select>
                </div>
                <div class="form-group form-inline">
                    <label style="width: 30px;">X:</label>
                    <input id="edit_x_pos" type="text" class="form-control" placeholder="position" required>
                </div>
                <div class="form-group form-inline">
                    <label style="width: 30px;">Y:</label>
                    <input id="edit_y_pos" type="text" class="form-control" placeholder="position" required>
                </div>
                <div class="form-group form-inline">
                    <label style="width: 30px;">Z:</label>
                    <input id="edit_z_pos" type="text" class="form-control" placeholder="position" required>
                </div>
                <div class="form-group">
                    <input id="edit_notes" type="text" class="form-control" placeholder="notes" required>
                </div>
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button id="edit_submit" type="button" class="btn btn-primary">Report</button>
            </div>
            <!-- <div class="modal-footer">
                <button id="edit_submit" type="button" class="btn btn-primary">Report</button>
            </div> -->

        </div>
    </div>
</div>

<script>
    populateOpts('edit_type');
</script>